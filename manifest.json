{
  "manifest_version": 3,
  "name": "Facebook Cleaner",
  "version": "1.0",
  "description": "Hide Reels, Ads, People You May Know, and Suggested Groups on Facebook.",
  "content_scripts": [{
    "matches": ["https://www.facebook.com/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
